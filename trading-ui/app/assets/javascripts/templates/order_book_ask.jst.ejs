<tr data-order="<%= index %>" data-price="<%= price %>" data-volume="<%= volume %>" class="<%= classes %> form__column-container form__column-container-value text--red">
  <td class="volume form__column order-book__column form__column-value text--green">
    <div class="text--green">
      <%= formatter.mask_fixed_volume(volume) %>
    </div>
  </td>
  <td class="price form__column order-book__column form__column-value text--green">
    <div class="text--green">
      <%= formatter.mask_fixed_price(price) %>
    </div>
  </td>
  <!--<td class="amount form__column order-book__column form__column-value text--green">
    <div class="text--green">
      <%= formatter.amount(volume, price) %>
    </div>
  </td>-->
</tr>
